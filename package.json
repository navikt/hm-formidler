    "test": "craco test",
    "eject": "craco eject",
    "client": "cross-env DECORATOR_URL=https://www.nav.no/dekoratoren/?context=samarbeidspartner yarn start",
    "server": "cross-env DECORATOR_URL=https://www.nav.no/dekoratoren/?context=samarbeidspartner ts-node-dev -P server/tsconfig.json -T -r dotenv/config server/server.ts",
    "server-docker-be": "cross-env DEBUG=express-http-proxy IDPORTEN_WELL_KNOWN_URL=http://host.docker.internal:8080/default/.well-known/openid-configuration TOKEN_X_WELL_KNOWN_URL=http://host.docker.internal:8080/default/.well-known/openid-configuration  API_URL=http://localhost:8083/api DECORATOR_URL=https://www.nav.no/dekoratoren/?context=samarbeidspartner ts-node-dev -P server/tsconfig.json -T -r dotenv/config server/server.ts",
    "mocked-be": "cross-env SOKNAD_URL=http://localhost:3000/hjelpemidler/digitalsoknadhjelpemidler NAIS_CLUSTER_NAME=labs-gcp SOKNAD_API_URL=http://host.docker.internal:4040/ API_URL=http://host.docker.internal:4040/api/ IDPORTEN_WELL_KNOWN_URL=http://host.docker.internal:8080/default/.well-known/openid-configuration TOKEN_X_WELL_KNOWN_URL=http://host.docker.internal:8080/default/.well-known/openid-configuration DECORATOR_URL=https://www.nav.no/dekoratoren/?context=samarbeidspartner ts-node-dev -P server/tsconfig.json -T -r dotenv/config server/server.ts",
    "devd": "concurrently --kill-others-on-fail \"yarn server-docker-be\" \"yarn client\"",
    "devoffline": "concurrently --kill-others-on-fail \"yarn mocked-be\" \"yarn client\"",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "up": "docker-compose up -d --build",
    "lint": "eslint --ext ts,tsx ."
